<h1>User profile page</h1>
<div>
  <h3>Liked movies</h3>
  {{#each liked}}
  <p>{{this.title}}</p>
  <img src="{{this.image}}" alt="{{this.title}} picture"> {{/each}}
</div>
<div>
  <h3>Hated movies</h3>
  {{#each hated}}
  <p>{{this.title}}</p>
  <img src="{{this.picture}}" alt="{{this.title}} picture"> {{/each}}
</div>
<div>
  <h3>Been there done that</h3>
  {{#each watched}}
  <p>{{this.title}}</p>
  <img src="{{this.image}}" alt="{{this.title}} picture"> {{/each}}
</div>